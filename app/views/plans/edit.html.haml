- content_for :head do
  = javascript_include_tag :plans

#editform
  =form_for @plan, :url => {:action=>:update, :controller=>:plans } do |f|
    =f.text_area :edit_text, :rows=>"14", :onkeyup=>"javascript:checkPlanLength()"
    %br
    #edit_fill_meter
      .fill_bar
        .full_amount
      .fill_percent
        0%
    =f.submit

